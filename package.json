{
  "name": "webdriverio-tests",
  "version": "0.1.0",
  "private": true,
  "devDependencies": {
    "@babel/cli": "^7.17.3",
    "@babel/core": "^7.17.4",
    "@babel/preset-env": "^7.16.11",
    "@babel/register": "^7.17.0",
    "@wdio/allure-reporter": "^7.16.14",
    "@wdio/cli": "^7.16.15",
    "@wdio/devtools-service": "^7.16.15",
    "@wdio/local-runner": "^7.16.15",
    "@wdio/mocha-framework": "^7.16.15",
    "@wdio/selenium-standalone-service": "^7.16.14",
    "@wdio/spec-reporter": "^7.16.14"
  },
  "scripts": {
    "wdio": "wdio run wdio.conf.js",
    "allure:history": "mkdir -p allure-report/history ./test/reports/allure-results/history && cp -a allure-report/history ./test/reports/allure-results/",
    "allure-report": "allure generate --clean ./test/reports/allure-results && allure open",
    "junit-report": "junit-viewer --results=./test/reports/junit-results --save=junit-results.html && open junit-results.html",
    "test-sauce": "wdio ./test/config/wdio.sauce.conf.js",
    "test-browserstack": "wdio ./test/config/wdio.browserstack.conf.js",
    "test-local": "npm run allure:history && wdio ./test/config/wdio.local.conf.js",
    "test-mobile": "wdio ./test/config/wdio.appium.conf.js"
  },
  "dependencies": {
    "allure-commandline": "^2.17.2",
    "fs-extra": "^10.0.0"
  },
  "main": "babel.config.js",
  "directories": {
    "test": "test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/galushchak/example.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/galushchak/example/issues"
  },
  "homepage": "https://github.com/galushchak/example#readme",
  "description": ""
}
